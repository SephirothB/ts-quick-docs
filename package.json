{
  "name": "ts-quick-docs",
  "version": "0.4.0",
  "description": "quick TypeScript documentation extractor",
  "bin": "dist/index.js",
  "main": "dist/index.js",
  "dependencies": {
    "typescript": "2.1"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/mocha": "2.2.39",
    "@types/node": "7.0.5",
    "@types/react": "15.0.6",
    "chai": "3.5.0",
    "mocha": "2.4.5",
    "ts-node": "2.0.0",
    "tslint": "4.4.2"
  },
  "scripts": {
    "all": "npm run clean && npm run compile && npm run mocha",
    "clean": "rm -rf dist",
    "compile": "tsc --project .",
    "lint": "tslint --project tsconfig.json",
    "prepublish": "npm run clean && npm run compile",
    "test": "mocha --compilers ts:ts-node/register"
  },
